package p1;
import java.io.*;
import java.net.*;

import java.net.ServerSocket;

public class Registry implements Runnable{
	private boolean debug = true;
	private String name = "Registry";
	Integer NUM_POSSIBLE_CONNECTIONS;
	ServerSocket ourServerSocket = null;
	
	public Registry(int maxConnections) {
		this.NUM_POSSIBLE_CONNECTIONS = maxConnections;
	}
	
	public void run(){
		if(debug) {System.out.println(this.name+": is Running!");}
		
	}
}
